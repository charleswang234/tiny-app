<!DOCTYPE html>
<html lang="en">

<%- include partials/_head.ejs%>

<body>
  <div class="container">
    <%- include partials/_header.ejs %>

    <table class="table">
      <!-- <tr>
        <td>URL</td>
        <td>shortened</td>
      </tr> -->

      <% for (shortened in urls) { %>
      <tr>
       <td class="well"><%= urls[shortened]%> &rarr; <%= shortened %> </td>
        <td>
          <a href="/urls/<%= shortened %>">Edit</a>
        </td>
        <td>
          <!-- <p>urls/<%= shortened %>/delete</p> -->
          <form method="POST" action="/urls/<%= shortened %>/delete">
            <input type="submit" value="DELETE">
          </form>
        </td>
      </tr>
      <% }; %>
    </table>
    <a href="/urls/new">New URL</a>
  </div>
</body>
</html>